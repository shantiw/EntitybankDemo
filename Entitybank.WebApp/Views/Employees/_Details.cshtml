<div id="details" data-plugin="renderer" class="invisible">
    <div class="row">
        <div class="col-sm-2">
            <label class="pull-right">Name:</label>
        </div>
        <div class="col-sm-4">
            <p>{{Name}}</p>
        </div>
        <div class="col-sm-2">
            <label class="pull-right">Gender:</label>
        </div>
        <div class="col-sm-4">
            <p>{{GenderName}}</p>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-2">
            <label class="pull-right">Birthdate:</label>
        </div>
        <div class="col-sm-4">
            <p>{{var m = moment(Birthdate); m.isValid() ? m.format('YYYY-MM-DD'):''}}</p>
        </div>
        <div class="col-sm-2">
            <label class="pull-right">Email:</label>
        </div>
        <div class="col-sm-4">
            <p>{{Email}}</p>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-2">
            <label class="pull-right">Tel:</label>
        </div>
        <div class="col-sm-4">
            <p>{{Tel}}</p>
        </div>
        <div class="col-sm-2">
            <label class="pull-right">Mobile:</label>
        </div>
        <div class="col-sm-4">
            <p>{{Mobile}}</p>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-2">
            <label class="pull-right">Addr:</label>
        </div>
        <div class="col-sm-10">
            <p>{{Addr}}</p>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-2">
            <label class="pull-right">Postcode:</label>
        </div>
        <div class="col-sm-4">
            <p>{{Postcode}}</p>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-2">
            <label class="pull-right">Comment:</label>
        </div>
        <div class="col-sm-10">
            <p>{{Comment}}</p>
        </div>
    </div>
</div>
